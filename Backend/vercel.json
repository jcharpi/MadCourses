{
  "version": 2,
  "builds": [
    {
      "src": "api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api.py"
    }
  ],
  "functions": {
    "api.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "MODEL": "all-MiniLM-L12-v2",
    "TOP_K": "5",
    "DB_PATH": "courses.db"
  }
}