{
	"functions": {
		"api/python/match.py": {
			"maxDuration": 30,
			"memory": 1024
		},
		"api/health.py": {
			"maxDuration": 10,
			"memory": 512
		}
	},
	"build": {
		"env": {
			"PYTHONDONTWRITEBYTECODE": "1",
			"PIP_NO_CACHE_DIR": "1"
		}
	},
	"env": {
		"MODEL": "all-MiniLM-L12-v2",
		"TOP_K": "5",
		"DB_PATH": "api/python/courses.db"
	}
}
