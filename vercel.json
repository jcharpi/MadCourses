{
	"functions": {
		"api/match.py": {
			"maxDuration": 30,
			"memory": 1024
		}
	},
	"build": {
		"env": {
			"PYTHONDONTWRITEBYTECODE": "1",
			"PIP_NO_CACHE_DIR": "1"
		}
	},
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api/$1"
		}
	],
	"env": {
		"MODEL": "all-MiniLM-L12-v2",
		"TOP_K": "5",
		"DB_PATH": "api/courses.db",
		"HF_API_TOKEN": "@hf_api_token"
	}
}
